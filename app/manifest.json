{
  "name": "Demo app",
  "version": "1",

  "manifest_version": 2,

  "icons": {"128": "dart_icon.png"},

  "app": {
    "background": {
      "scripts": ["background.js"]
    }
  },

  "permissions": [
    "alarms",
    "audio",
    { "fileSystem": ["write", "retainEntries", "directory"] },
    "idle",
    "syncFileSystem",
    "system.cpu",
    "system.memory",
    { "socket" : ["tcp-connect", "tcp-listen"] },
    "tts"
  ]
}
